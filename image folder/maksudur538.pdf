----------------------------------------------------css----------------------------------------------------------css-------------------------------------------------
nav ul li{
    display: inline-block;
    justify-content: flex-end;
    margin-right: 40px;
   
}
:root{
    --praymary-color:#edf2fc;
    --secondary-color:#212121;
      
}
.drack-thim{
    --praymary-color:#212121;
    --secondary-color:#edf2fc;
}
 
nav ul{
    flex: 1;
    text-align: right;
}
nav ul li a{
   text-decoration: none;
   
   
}
nav ul li a:hover{
    color:var(--secondary-color);
    transition: 1s;
}
nav{
    display: flex;
    justify-content: space-between;
    margin: 15px;
    background-color:rgb(248, 208, 133);
    padding-left: 10px;
    padding-right: 10px;
    text-align: center;
    
    
}
body{
    background-color: var(--praymary-color); 
} 
#logo{
    height: 50px;
    width: 90px;
    
}
.text{
    margin: 15% 0% 0% 10%;
   color:var(--secondary-color)
}
#cv{
    display: block;
}
i{
    margin: 30px 10px 10px 10px;

}
.templeat{
    display: flex;
    
}
#pattern{
    height: 400px;
    width: 350px;
    position: absolute;
    transition:  3s;
    
    
}
#girl{
    height: 400px;
    width: 250px;
   position: absolute;
   margin: 0% 0% 0% 5%;
   transition:  1s;
   
}
#girl:hover,#pattern:hover{
    transform: translateX(5%);
    transform: translateX(-10%);
}
.vola{
    margin-left: 90px;
    margin-top: 6.75%;
}

#moon{
    height: 20px;
    width: 20px;
    cursor: pointer;
}
i{
    color: aqua;
}
strong{
    color: red;
}
.two li{
    display:inline-block;
}
 a{
    text-decoration: none;
    
    margin-right: 10px ;
    color: var(--praymary-color);
}
.one{
    margin-top: 36px;
    display: flex;
    background-color:var(--secondary-color);
   color: var(--praymary-color);
   justify-content:space-between;
   padding: 50px;
}
.three{
    background-color: tomato;
    border-radius: 3px;
    height: 30px;
    width: 70px;
    text-align: center;
   margin: 10px 0px 10px 100px;
   padding-top: 9px;
  
}
#three{
   
    color:var(--praymary-color);
    
    
    
}


.templeat{
    background-color: var(--praymary-color);
    
}



button{
    border: 2px solid tomato;
    color: tomato;
    border-radius: 2px;
    padding: 5px 20px;
}
button:hover{
    background-color: tomato;
    color:var(--praymary-color);
}
.flex{
    display: flex;
    
}
.mashud{
    margin: 10px;
    color: var(--secondary-color);
}
#two{
    margin-left: 13%;
    
}
